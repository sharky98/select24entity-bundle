<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

  <services>
    <service id="brunops_select24entity.select24entity_type" class="Brunops\Select24EntityBundle\Form\Type\Select24EntityType">
      <tag name="form.type" alias="brunops_select24entity" />
      <argument type="service" id="doctrine.orm.entity_manager" />
      <argument type="service" id="router" />
      <argument>%brunops_select24entity.minimum_input_length%</argument>
      <argument>%brunops_select24entity.page_limit%</argument>
      <argument>%brunops_select24entity.allow_clear%</argument>
      <argument>%brunops_select24entity.delay%</argument>
      <argument>%brunops_select24entity.language%</argument>
      <argument>%brunops_select24entity.cache%</argument>
    </service>
  </services>

</container>
